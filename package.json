{
  "name": "icecreamparlor-orderservice",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "imports": {
    "#src/*": "./src/*.ts",
    "#test/*": "./test/*.ts",
    "#prod_src/*": "./dist/*.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test-nopretty": "better-npm-run test-nopretty",
    "build": "tsc && find dist -name '*.js'|xargs sed -i -e 's/#src/#prod_src/g'",
    "start": "better-npm-run prod"
  },
  "betterScripts": {
    "dev": {
      "command": "ts-node-esm ./bin/www.ts | pino-pretty",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "test": {
      "command": "jest --runInBand | pino-pretty",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-nopretty": {
      "command": "jest --runInBand",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "prod": {
      "command": "node ./dist/www",
      "env": {
        "NODE_ENV": "prod"
      }
    }
  },
  "dependencies": {
    "assert": "2.1.0",
    "better-npm-run": "0.1.1",
    "cors": "2.8.5",
    "dayjs": "1.11.19",
    "debug": "4.4.3",
    "dotenv": "17.2.3",
    "express": "5.2.1",
    "http-errors": "2.0.0",
    "knex": "3.1.0",
    "mysql": "2.18.1",
    "mysql2": "3.15.3",
    "pino": "10.1.0",
    "rabbitmq-client": "5.0.8",
    "test": "3.3.0",
    "typescript": "5.9.3",
    "zod": "4.1.12",
    "zod-validation-error": "5.0.0"
  },
  "devDependencies": {
    "@testcontainers/mysql": "11.7.2",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.5",
    "@types/jest": "30.0.0",
    "@types/mysql": "2.15.27",
    "@types/node": "24.10.0",
    "@types/supertest": "6.0.3",
    "iconv-lite": "0.7.0",
    "jest": "30.2.0",
    "pino-pretty": "13.1.2",
    "supertest": "7.1.4",
    "testcontainers": "11.7.2",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsx": "4.20.6"
  }
}
